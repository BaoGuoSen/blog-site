{
  "name": "blog-site",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "npm run lint:ci && vite",
    "dev:mock": "npm run lint:ci && (ts-node -T ./server/index.ts & vite)",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "lint:ci": "npm run eslint:ci && npm run stylelint:ci",
    "eslint:ci": "eslint --cache --ext .tsx,.ts ./src",
    "stylelint:ci": "stylelint ./src/**/*.less --custom-syntax postcss-scss",
    "lint:fix": "npm run stylelint:fix && npm run eslint:fix",
    "stylelint:fix": "stylelint --fix ./src/**/*.less --custom-syntax postcss-scss",
    "eslint:fix": "eslint --cache --ext .tsx,.ts ./src --fix",
    "push:pre": "npm run lint:fix && npm run prettier"
  },
  "dependencies": {
    "eslint": "^8.17.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.6",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "^1.3.0",
    "colors-console": "^1.0.3",
    "cors": "^2.8.5",
    "eslint": "^8.17.0",
    "eslint-plugin-react": "^7.30.0",
    "express": "^4.18.1",
    "mockjs": "^1.1.0",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.0",
    "stylelint": "^14.9.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
